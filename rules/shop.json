{
  "shopifyActiveConsent": {
    "detectors": [{
      "presentMatcher": {
        "type": "css",
        "target": { "selector": "section#active-consent" }
      },
      "showingMatcher": {
        "type": "css",
        "target": { "selector": "section#active-consent" }
      }
    }],
    "methods": [{
      "name": "DO_CONSENT",
      "action": {
        "type": "click",
        "target": {
          "selector": "section#active-consent button[data-component-name='reject']",
          "textFilter": "Reject all"
        }
      }
    }]
  }
}
